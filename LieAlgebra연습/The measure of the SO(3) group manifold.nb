Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{" ", 
   RowBox[{
    RowBox[{"R", "(", 
     RowBox[{
      OverscriptBox["n", "\[RightVector]"], ",", "\[Theta]"}], ")"}], " ", 
    "~", " ", 
    RowBox[{"R", "(", 
     RowBox[{
      OverscriptBox["m", "\[RightVector]"], ",", " ", "\[Theta]"}], ")"}], 
    " ", "~", " ", 
    RowBox[{"R", "(", 
     RowBox[{
      SubscriptBox[
       OverscriptBox["e", "\[RightVector]"], "z"], ",", "\[Theta]"}], 
     ")"}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"d65fd2be-ad04-4886-9bd9-fb3b6d388c35"]], \
"Text",
