Cell[BoxData[
 FormBox[
  RowBox[{"\[DoubleStruckR]", "=", 
   RowBox[{
    RowBox[{"|", 
     RowBox[{"r", "-", "R"}], "|"}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
